{
    
    "explorer.enableDragAndDrop": true,

    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#ff3bad",
        "terminalCursor.foreground": "#ff00b3",
    },

    "emmet.syntaxProfiles": { "javascript": "jsx" },
    "emmet.includeLanguages": { "javascript": "javascriptreact" },
  

    // "eslint.validate": ["javascript", "typescript", "typescriptreact"],


    "editor.cursorWidth": 3,
    "workbench.iconTheme": "material-icon-theme",
    // Configs para a parte de cima colada no topo
    // "window.titleBarStyle": "native", // "native" on Linux, "custom" on Windows

    


    "window.menuBarVisibility": "toggle",
    "editor.mouseWheelZoom": true,
    "editor.fontSize": 18,
    // "editor.fontFamily": "Jetbrains Mono",
    // "editor.fontFamily": "'Victor Mono Medium'",
    "editor.fontFamily": "Dank Mono",
    // "editor.fontFamily": "'Fira Code iScript'",
    
    "editor.lineHeight": 30,
    "editor.fontLigatures": true,
    "editor.fontWeight": "600",
    "editor.tabSize": 2,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "editor.parameterHints.enabled": false,
    "breadcrumbs.enabled": true,
    "editor.renderLineHighlight": "gutter",
    "explorer.compactFolders": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
    //Eslint
    "[javascript]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true,
        }
    },
    "[javascriptreact]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true,
        }
    },
    "explorer.confirmDelete": false,
    "material-icon-theme.folders.associations": {
        "infra": "app",
        "entities": "class",
        "schemas": "class",
        "typeorm": "database",
        "repositories": "mappings",
        "http": "container",
        "migrations": "tools",
        "modules": "components",
        "implementations": "core",
        "dtos": "typescript",
        "fakes": "mock",
        "websockets": "pipe",
        "protos": "pipe",
        "grpc": "pipe",
        "deps": "Download",
        "priv": "Guard",
        "interfaces": "Less",
        "share": "Export"
    },
    "material-icon-theme.files.associations": {
        "*.controller.ts": "nest-controller",
        "*.module.ts": "nest-module",
        "*.service.ts": "nest-service",
        "*.middleware.ts": "nest-middleware",
        "*.filter.ts": "nest-filter",
        "*.pipe.ts": "nest-pipe",
        "*.gateway.ts": "nest-gateway",
        "*.decorator.ts": "nest-decorator",
        "*.resolver.ts": "nest-resolver",
        "*.input.ts": "nest-input",
        "*.guard.ts": "nest-guard",
        "ormconfig.json": "database",
        "tsconfig.json": "tune",
        "*.proto": "3d",
        "*.beam": "Assembly",
        "styles.ts": "sass",
        "global.ts": "sass",
        "router.ex": "Routing"
    },
    "git.autofetch": true,
    "typescript.updateImportsOnFileMove.enabled": "never",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "explorer.confirmDragAndDrop": false,
    "sync.gist": "accd5bcc44386b0928938f95727bec46",
    "files.exclude": {
        "**/.git": true,
        "**/.next": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/deps": true,
        "node_modules": true,
        ".directory": true,
        "_build": true,
        ".elixir_ls": true
    },
    "terminal.integrated.cursorStyle": "block",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontWeight": "bold",
    // "terminal.integrated.fontFamily": "'DroidSansMono Nerd Font'",
    "terminal.integrated.fontFamily": "'Fira Code iScript'",

    "emmet.syntaxProfiles": { "javascript": "jsx" },
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "extensions.autoUpdate": true,
    "editor.wordWrap": "on",
    "vscodeGoogleTranslate.preferredLanguage": "English",
    "terminal.integrated.cursorBlinking": true,
    "editor.cursorBlinking": "phase",
    "workbench.activityBar.visible": true,
    "terminal.integrated.rendererType": "dom",
    "material-icon-theme.folders.color": "#65309b",
    "liveServer.settings.donotShowInfoMsg": true,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "tabnine.experimentalAutoImports": true,
    "workbench.colorTheme": "Omni",
    "window.zoomLevel": 0.8,

